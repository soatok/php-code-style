{
  "name": "soatok/code-style",
  "description": "Custom PHP-CS-Fixer config for Soatok's code style",
  "type": "library",
  "license": "ISC",
  "require": {
    "php": ">=8.1",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Soatok\\CodeStyle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Soatok\\CodeStyle\\Tests\\": "tests/"
    }
  }
}